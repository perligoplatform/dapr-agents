{
  "name": "@dapr/agents",
  "version": "0.1.0",
  "description": "TypeScript implementation of Dapr Agents - A Framework for Agentic AI Systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": ["dapr", "agents", "workflows", "ai", "typescript"],
  "author": "Dapr Authors <dapr@dapr.io>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "dev": "tsx src/index.ts"
  },
  "dependencies": {
    "@dapr/dapr": "^3.5.0",
    "openai": "^4.52.0",
    "zod": "^3.23.8",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "tsx": "^4.16.2",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dapr/dapr-agents.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/dapr/dapr-agents/issues"
  }
